<h1>Listing sessions</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Start time</th>
    <th>End time</th>
  </tr>

<% @sessions.each do |session| %>
  <tr>
    <td><%=h session.name %></td>
    <td><%=h session.start_time %></td>
    <td><%=h session.end_time %></td>
    <td><%= link_to 'Show', barcamp_session_path(@barcamp, session) %></td>
    <td><%= link_to 'Edit', edit_barcamp_session_path(@barcamp, session) %></td>
    <td><%= link_to 'Destroy', barcamp_session_path(@barcamp, session), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Add a session', new_barcamp_session_path(@barcamp) %>